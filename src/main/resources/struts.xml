<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.devMode" value="true" />

	<package name="/api" extends="struts-default">
		<action name="index" class="com.cpj.swagger.support.struts2.ApiAction">
			<result name="success">/swagger/api.jsp</result>
		</action>
		
		<action name="apis" class="com.cpj.swagger.support.struts2.ApiAction" method="queryApi" />
	</package>
</struts>
